{
  "name": "repo-configs",
  "version": "0.3.2",
  "description": "IPFS repo configs",
  "main": "dist/configs-node.js",
  "module": "dist/configs-es.js",
  "repository": "https://github.com/crypto-io/repo-configs",
  "author": "vandeurenglenn <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "node test.js"
  },
  "keywords": [
    "ipfs",
    "repo",
    "config",
    "blockstore",
    "bootstrap"
  ],
  "backed": {
    "bundles": [
      {
        "src": "src/index.js",
        "dest": "dist/configs.js",
        "format": "es",
        "plugins": {
          "json": {}
        }
      },
      {
        "src": "src/index.js",
        "dest": "dist/configs.js",
        "format": "cjs",
        "plugins": {
          "json": {}
        }
      }
    ]
  },
  "dependencies": {
    "peer-id": "^0.10.6"
  },
  "devDependencies": {
    "rollup-plugin-json": "^2.3.0",
    "tape": "^4.9.0"
  }
}
